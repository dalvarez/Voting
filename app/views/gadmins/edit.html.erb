<h1>Editing gadmin</h1>

<% form_for(@gadmin) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :gaid %><br />
    <%= f.text_field :gaid %>
  </p>
  <p>
    <%= f.label :uid %><br />
    <%= f.text_field :uid %>
  </p>
  <p>
    <%= f.label :gid %><br />
    <%= f.text_field :gid %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @gadmin %> |
<%= link_to 'Back', gadmins_path %>