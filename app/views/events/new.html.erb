<h1>New events</h1>
 
<% form_for(@events) do |f| %>
  <%= f.error_messages %>
 
  <p>
    <%= f.label :group_id %><br />
    <%= f.text_field :group_id %>
  </p>
  <p>
    <%= f.label :ename %><br />
    <%= f.text_field :ename %>
  </p>
  <p>
    <%= f.label :edescription %><br />
    <%= f.text_field :edescription %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.text_field :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.text_field :end_date %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>
 
<%= link_to 'Back', events_path %>