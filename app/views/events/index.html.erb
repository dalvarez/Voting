<h1>Listing events</h1>

<table>
  <tr>
    <th>GroupID</th>
    <th>Event Name</th>
    <th>Event Description</th>
    <th>Start date</th>
	<th>End date</th>
  </tr>
<% @events.each do |events| %>
  <tr>
  	<td><%=h events.group_id %>||</td>
    <td><%=h events.ename %>||</td>
	<td><%=h events.edescription %>||</td>
	<td><%=h events.start_date %>||</td>
	<td><%=h events.end_date %>||</td>
  </tr>
<% end %>
</table>

<br />
